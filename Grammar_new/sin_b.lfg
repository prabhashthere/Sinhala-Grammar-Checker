DEMO   SINHALA   TEMPLATES (1.0)

OPT-TRANS(P) = { (^ PRED) = 'P<(^SUBJ)(^OBJ)>'
	          | (^ PRED) = 'P<(^SUBJ)>'
		  | (^ PRED)='P<(^SUBJ)(^OBJ2)(^OBJ)>'
		    {(^OBJ CASE) = acc | (^OBJ CASE) = nom}
		  | (^ PRED) = 'P' }.

TRANS(P) = { (^ PRED) = 'P<(^SUBJ)>'
		  |  (^ PRED) = 'P' }.

DI-TRANS(P) = { (^ PRED) = 'P<(^SUBJ)(^OBJ)>'
                {(^OBJ CASE) = acc | (^OBJ CASE) = nom}
		  |  (^ PRED) = 'P' }.

TRY-TRANS(P ) = { (^ PRED)='P<(^SUBJ)(^OBJ2)(^OBJ)>'
                 | (^ PRED) = 'P'
                 |
                  (^ PRED) = 'P<(^SUBJ)(^OBJ)>'

                 }.


TENSE(T) = (^ TENSE) = T.

PERS(P T L) =   {(^SUBJ NUM) = T
                 (^SUBJ PERS) = P
                 (^SUBJ CASE)=nom
                 {(^SUBJ LIV) = L | (^ UNGRAMMATICAL) = {Number_Mismatch_Due_to_Inanimacy} }
                 |
                 (^SUBJ NUM) = T
                 (^SUBJ LIV) = L
                 (^SUBJ CASE)=nom
                 {(^SUBJ PERS) = P | (^ UNGRAMMATICAL) = {Person_Mismatch} }
                 |
                 (^SUBJ LIV) = L
                 (^SUBJ PERS) = P
                 {(^SUBJ NUMB) = T | (^ UNGRAMMATICAL) = {Number_Mismatch}}
                 (^SUBJ CASE)=nom

                 |
                 (^SUBJ LIV) = L
                 (^SUBJ PERS) = P
                  (^SUBJ NUM) = T

                 }.

PERS2(Pcase Scase P T L) =   {(^SUBJ NUM) = T
                   (^SUBJ PERS) = P
                   {(^SUBJ CASE)=nom |(^SUBJ CASE)=acc}
                   {(^SUBJ LIV) = L | (^ UNGRAMMATICAL) = {Animacy_Mismatch} }
                   (^OBJ CASE)=Pcase
                   (^OBJ2 CASE)=Scase
                   |
                   (^SUBJ NUM) = T
                   (^SUBJ LIV) = L
                   {(^SUBJ CASE)=nom |(^SUBJ CASE)=acc}
                   {(^SUBJ PERS) = P | (^ UNGRAMMATICAL) = {Person_Mismatch} }
                   (^OBJ CASE)=Pcase
                   (^OBJ2 CASE)=Scase
                   |
                   (^SUBJ LIV) = L
                   (^SUBJ PERS) = P
                   {(^SUBJ CASE)=nom |(^SUBJ CASE)=acc}
                   {(^SUBJ NUMB) = T | (^ UNGRAMMATICAL) = {Number_Mismatch}}
                   (^OBJ CASE)=Pcase
                   (^OBJ2 CASE)=Scase
                   |
                   (^SUBJ LIV) = L
                   (^SUBJ PERS) = P

                    (^SUBJ NUM) = T
                    (^OBJ CASE)=Pcase
                   (^OBJ2 CASE)=Scase
                   |
                   (^SUBJ LIV) = L
                  (^SUBJ PERS) = P
                   (^SUBJ NUM) = T
                   {(^OBJ CASE)=Pcase | (^ UNGRAMMATICAL) = {Object_case_Mismatch}  }
                  (^OBJ2 CASE)=Scase
                  |
                 (^SUBJ LIV) = L
                (^SUBJ PERS) = P
                 (^SUBJ NUM) = T
                 (^OBJ CASE)=Pcase
                {(^OBJ2 CASE)=Scase| (^ UNGRAMMATICAL) = {Secondary_Object_case_Mismatch}  }
                   }
                   .


VERVF(F) = (^VFORM) =F.


